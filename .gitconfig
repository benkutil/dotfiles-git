[user]
  email = ben@make-things.com
  name = Ben Kutil
[core]
  editor = subl -w
  whitespace=fix,-indent-with-non-tab,trailing-space,space-before-tab,cr-at-eol
  autocrlf = input
  excludesfile = /Users/ktb/.gitignore
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = true
  pager = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  header = white
  added = green
  changed = yellow bold
  untracked = white dim
  renamed = pink
[svn]
  authorsfile = /Users/ktb/.gitusers
[push]
  default = matching
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
